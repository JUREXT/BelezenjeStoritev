<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jurext.belezenje_storitve.belezenje_storitev">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_ikona"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Prikaz_storitev"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Shrani_storitev"
            android:screenOrientation="portrait" />
        <activity
            android:screenOrientation="portrait"
            android:name=".Nastavitve"
            android:label="@string/title_activity_nastavitve">
        </activity>
    </application>

</manifest>